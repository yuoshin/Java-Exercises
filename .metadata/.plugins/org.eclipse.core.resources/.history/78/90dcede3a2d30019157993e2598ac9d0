package testla;

import java.util.*;

class testlaMain {
	
	public static void main(String[] args) {
		printNumber(10);
	}
		public static void printNumber(int number) {
			if(number <= 100) {
				System.out.println(number);
				number++;
				printNumber(number);			//Note: method calling itself
			}else {
				System.exit(0);
			}
		}
}
