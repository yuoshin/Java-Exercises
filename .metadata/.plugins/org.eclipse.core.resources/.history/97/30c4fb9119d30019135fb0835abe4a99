package rough;

public class RoughMain {
	public static void main(String[] args) {
		int numberMax = Integer.MAX_VALUE;
		int numberMin = Integer.MIN_VALUE;
		long[] arr = {-535346242332543534l,43, 724,9543};
		
		for(int num:arr) {
			if(num < numberMin) {
				numberMin = num;
			}
		}
		System.out.println(numberMax);
		System.out.println(numberMin);
	}
}